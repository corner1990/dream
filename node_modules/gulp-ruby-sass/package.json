  "dependencies": {
    "convert-source-map": "^1.0.0",
    "dargs": "^2.0.3",
    "cross-spawn": "^5.0.0",
    "each-async": "^1.0.0",
    "escape-string-regexp": "^1.0.3",
    "glob": "^7.0.3",
    "glob2base": "0.0.12",
    "gulp-util": "^3.0.4",
    "md5-hex": "^1.0.2",
    "object-assign": "^4.0.1",
    "os-tmpdir": "^1.0.0",
    "path-exists": "^2.0.0",
    "rimraf": "^2.2.8"
  },
  "devDependencies": {
    "gulp": "^3.8.11",
    "gulp-sourcemaps": "^2.2.0",
    "mocha": "*",
    "vinyl-file": "^2.0.0",
    "xo": "^0.16.0"
  },
  "xo": {
    "rules": {
      "brace-style": [
        "error",
        "stroustrup",
        {
          "allowSingleLine": false
        }
      ]
    }
  }
